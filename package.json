{
  "name": "channex-knowledge-graph",
  "version": "1.0.0",
  "description": "Knowledge graph builder for Channex.io API documentation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "parse": "tsx scripts/parse-docs.ts",
    "test": "jest",
    "test:setup": "tsx scripts/test-setup.ts",
    "query": "tsx scripts/query-examples.ts",
    "agent:demo": "tsx scripts/agent-demo.ts",
    "mcp:build": "tsc src/mcp-server.ts --outDir dist",
    "mcp:start": "node dist/mcp-server.js",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist node_modules",
    "install:clean": "npm run clean && npm install",
    "sync:neo4j": "tsx scripts/sync-to-neo4j.ts",
    "query:neo4j": "tsx scripts/neo4j-queries.ts",
    "neo4j:up": "docker-compose up -d",
    "neo4j:down": "docker-compose down",
    "neo4j:logs": "docker-compose logs -f neo4j",
    "clear:supabase": "tsx clear-supabase.ts",
    "clear:neo4j": "tsx scripts/clear-neo4j.ts",
    "build:mcp": "tsc src/mcp-server.ts --outDir dist",
    "start:mcp": "node dist/mcp-server.js"
  },
  "keywords": [
    "channex",
    "knowledge-graph",
    "api",
    "documentation"
  ],
  "author": "Supahost",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@supabase/supabase-js": "^2.39.3",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.1",
    "marked": "^12.0.0",
    "neo4j-driver": "^5.28.1",
    "openai": "^4.28.0",
    "p-queue": "^8.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
